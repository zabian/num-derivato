<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>zpliki</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ImageMaker</display-name>
    <servlet-name>ImageMaker</servlet-name>
    <servlet-class>util.ImageMaker</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageMaker</servlet-name>
    <url-pattern>/imageMaker</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ImageMaker1</display-name>
    <servlet-name>ImageMaker1</servlet-name>
    <servlet-class>web.imageMaker</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageMaker1</servlet-name>
    <url-pattern>/imageMaker1</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Serwlet generujący plik csv ze współrzędnymi punktów</description>
    <display-name>PointsFileMaker</display-name>
    <servlet-name>PointsFileMaker</servlet-name>
    <servlet-class>util.PointsFileMaker</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PointsFileMaker</servlet-name>
    <url-pattern>/PointsFileMaker</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PointsFileMaker</servlet-name>
    <url-pattern>/punkty.csv</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ChartMaker</display-name>
    <servlet-name>ChartMaker</servlet-name>
    <servlet-class>util.ChartMaker</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChartMaker</servlet-name>
    <url-pattern>/wykres.png</url-pattern>
  </servlet-mapping>
</web-app>